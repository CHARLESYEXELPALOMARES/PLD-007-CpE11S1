#include <iostream>
#include <string>
using namespace std;

int main() {
    string inputString;
    int total = 0;

    for (int i = 0; i < 4; i++) {
        cout << "Enter an integer as a string: ";
        cin >> inputString; // Input string
        total += stoi(inputString); // Convert to integer and add to total
    }

    cout << "The total is: " << total << endl; // Output total
    return 0;
}
