#include <iostream>
using namespace std;

int main() {
    // Loop through numbers from 1 to 7
    for (int i = 1; i <= 7; i++) {
        cout << i; // Print the current number
        if (i < 7) {
            cout << ", "; // Print a comma after each number except the last
        }
    }
    cout << endl; // Move to a new line after printing the sequence
    return 0; 
}
